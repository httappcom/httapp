<apex:page standardController="Patient__c" extensions="PatientExtension" >
<apex:form >
<apex:includeScript value="{!$Resource.jQuery}" />
<apex:includeScript value="{!$Resource.jQueryTwisty}" /> 
<apex:includeScript value="{!$Resource.sfscript}" /> 
<apex:stylesheet value="{!$Resource.sfstyles}"/>

<div class="wrapper">
    <div class="col3">
    
        <div class="sidebardetails">
        </div>
        <div class="vfButtonBarDark">
            <div>
                <span class="vfButtonBarDarkheader">Tasks</span> 
               
                <apex:selectList value="{!selectedTaskOption}" size="1" id="sidebardropdown" styleClass="sidebardropdown" onChange="onChangeCC(this.options[this.selectedIndex].value);">
                    <apex:selectOptions value="{!taskOptions}" />
                    <apex:actionfunction action="{!searchTask}" name="onChangeCC" reRender="taskResult"/>
               </apex:selectList>
                <!--<select name="dropdown" size="1" >
                    <option value="allopen">All Open</option>
                    <option value="allclosed">All Closed</option>
                    <option value="myopen" selected="selected">My Open</option>
                    <option value="myclosed">My Closed</option>
                 </select>-->
            </div>
        </div>
        <apex:outputPanel id="taskResult">        
        <div class="sidebardetails">
           <apex:repeat value="{!tasks}" var="t">        
            <div class="item"><a href="#"><apex:inputfield value="{!t.Subject}"></apex:inputfield></a> <apex:inputfield value="{!t.Status}"></apex:inputfield></div>
            <div class="optionindent">      
                <div class="label">Owner:</div>
                <div class="itemsmall"><a href="#"><apex:inputfield value="{!t.Owner.Name}"></apex:inputfield></a></div> 
                <div class="label">Due:</div>
                <div class="itemsmall"><a href="#"><apex:inputfield value="{!t.ActivityDate}"></apex:inputfield></a></div>
            </div>
            </apex:repeat>
            <div style="clear:both"></div>
        </div>
        </apex:outputPanel>
        <br /><br />
        
        <div class="vfButtonBarDark">
            <div>
                <span class="vfButtonBarDarkheader">Messaging</span>
            </div>
        </div>  
        <div class="sidebardetails">
            <apex:repeat value="{!messages}" var="msg">
                <div class="labelnextrow"><apex:inputfield value="{!msg.Name}"></apex:inputfield></div>
                <div class="item"><a href="#">This will be the summary of the message</a></div>
            </apex:repeat>
        </div><br /><br />  
        
        <div class="vfButtonBarDark">
            <div>
                <span class="vfButtonBarDarkheader">Care Team Members</span>
            </div>
        </div>  
        <div class="sidebardetails">
            <div class="ctmwrap">
                <div class="ctmimage">
                <apex:image id="avatar1" value="{!$Resource.avatar}" width="45" height="45" /></div>
                <div class="label">John Smith:</div>
                <div class="item"><a href="#">Assign Task</a></div>
            </div>
            
            <div class="ctmwrap">
                <div class="ctmimage"><apex:image id="avatar2" value="{!$Resource.avatar}" width="45" height="45" /></div>
                <div class="label">Sam Jones:</div>
                <div class="item"><a href="#">Assign Task</a></div>
            </div>
            
        </div><br /><br />      
        
    </div>
    <div class="col2">
        <div class="twisty open">   
            <div class="orangeheader">
                <span class="orangeheadertitle"><a href="/{!patient.id}" target="_blank" ></a></span>

                <span class="treatmentwrapper">
                    <span class="treatmenttitle"> {!treatmentCurNumber} of {!treatmentTotCount} Treatments</span>
                    <span class="treatmentboxeswrapper">
                        <span class="treatmentboxon"> </span>
                        <span class="treatmentbox"> </span>
                        <span class="treatmentbox"> </span>
                        <span class="treatmentbox"> </span>
                    </span>
                    <span class="treatmenttitle">{!phaseCount } of 4 Phases</span>
                    <span class="treatmentboxeswrapper">
                        <span class="treatmentboxon"> </span>
                        <span class="treatmentbox"> </span>
                        <span class="treatmentbox"> </span>
                        <span class="treatmentbox"> </span>         
                    </span>
                </span>
            </div><br />
            <div class="fieldwrapper">
                <div class="fieldwrap"><div class="label"><apex:commandbutton action="{!doSave}"   value="Save Patient "></apex:commandbutton> </div></div>   
            </div>
            <div class="fieldwrapper">
                <div class="fieldwrap"><div class="label">Suffix: </div> 
                <div class="item"><apex:inputfield value="{!patient.Salutation__c}"></apex:inputfield></div></div>
                <div class="fieldwrap"><div class="label">First Name:</div> 
                    <div class="item">
                        <a href="/{!patient.id}" target="_blank" ><apex:inputText value="{!firstName}" ></apex:inputText></a>
                    </div>
                </div>
                <div class="fieldwrap"><div class="label">Last Name:</div> 
                    <div class="item">
                       <a href="/{!patient.id}" target="_blank" ><apex:inputText value="{!lastName}" ></apex:inputText> </a>
                    </div>
                </div>                
                <div class="fieldwrap">
                    <div class="label">Email:</div> 
                    <div class="item"><apex:inputfield value="{!patient.Email__c}"></apex:inputfield></div>
                </div>
                
                <div class="fieldwrap">
                    <div class="label">Primary Phone:</div> 
                    <div class="item"><apex:inputfield value="{!patient.Phone__c}"></apex:inputfield></div>
                </div>
                
                <div class="fieldwrap">
                    <div class="label">Alternate Phone:</div> 
                    <div class="item"><apex:inputfield value="{!patient.Mobile__c}"></apex:inputfield></div>
                </div>
                
                <div class="fieldwrap">
                    <div class="label">Street:</div> 
                    <div class="item"><apex:inputfield value="{!patient.Street__c}"></apex:inputfield></div>
                </div>      
                              
                <div class="fieldwrap">
                    <div class="label">City:</div> 
                    <div class="item"><apex:inputfield value="{!patient.City__c}"></apex:inputfield></div>
                </div>
                
                <div class="fieldwrap">
                    <div class="label">State/Province:</div> 
                    <div class="item"><apex:inputfield value="{!patient.State__c}"></apex:inputfield></div>
                </div>
                
                <div class="fieldwrap">
                    <div class="label">Zip/Postal Code:</div> 
                    <div class="item"><apex:inputfield value="{!patient.Postal_Code__c}"></apex:inputfield></div>
                </div>
                
                <div class="fieldwrap">
                    <div class="label">Country:</div> 
                    <div class="item"><apex:inputfield value="{!patient.Country__c}"></apex:inputfield></div>
                </div>
                
                <div class="fieldwrap">
                    <div class="label">Key Contact:</div> 
                    <div class="item">(insert custom object)</div>
                </div>
                
                <div class="fieldwrap">
                    <div class="label">Preferred Language:</div> 
                    <div class="item"><apex:inputfield value="{!patient.Preferred_Language__c}"></apex:inputfield></div>
                </div>
                
                <div class="fieldwrap"><div class="label">Last Communication:</div> <div class="item">(insert custom object)</div></div>
                <div class="fieldwrap">
                    <div class="label">Owner:</div> 
                    <div class="item"><apex:inputfield value="{!patient.Owner.Name}"></apex:inputfield></div>
                </div>
            
            </div>
        </div>
        
        <div class="twisty open">       
        <div class="orangeheader">
            <span class="orangeheadertitle">Treatment</span>
            <span class="orangeheaderoption"><a href="#" style="color:#fff"></a></span>
        </div><br />
        
                
        <div class="treatmentfieldwrapper">
             <apex:commandbutton action="{!newTreatment}"   value="New Treatment"></apex:commandbutton>        
             <apex:commandbutton action="{!saveTreatment}"   value="Save Treatment"></apex:commandbutton>
            <div class="treatmentform">
                <div class="fieldwrapper">
                    <div class="fieldwrap">
                        <div class="label">Treatment Event #</div> 
                        <div class="item">
                            <a href="/{!treatment.id}" target="_blank" > {!treatment.Name}</a>
                        </div>
                    </div>
                    
                    <div class="fieldwrap">
                        <div class="label">Next Follow Up:</div> 
                        <div class="item">(insert custom object)</div>
                    </div>
                    
                    <div class="fieldwrapfull">
                        <div class="labellong">Patient Not Committed Reason:</div> 
                        <div class="item">(insert custom object)</div>
                    </div>
                </div>
            </div><div class="fieldheading">Staging Process</div>
            <div class="treatmentform">
                <div class="fieldwrapper">                  
                    <div class="fieldwrap"><div class="label">Phase</div> <div class="item"><apex:inputfield value="{!treatment.Phase__c}"></apex:inputfield></div></div>
                    <div class="fieldwrap"><div class="label">Stage</div> <div class="item"><apex:inputfield value="{!treatment.Stage__c}"></apex:inputfield></div></div>
                    <div class="fieldwrapfull"><div class="label">Sub-Stage</div> <div class="item"><apex:inputfield value="{!treatment.Sub_Stage__c}"></apex:inputfield></div></div>
                </div>
            </div><div class="fieldheading">Procedure Information</div>
            <div class="treatmentform">
                <div class="fieldwrapper">
                    <div class="fieldwrap"><div class="label">Primary Provider</div> <div class="item"><apex:inputfield value="{!treatment.Primary_Provider__c}"></apex:inputfield></div></div>
                    <div class="fieldwrap"><div class="label">Procedure Date</div> <div class="item"><apex:inputField value="{!treatment.Procedure_Date__c}"/></div></div>
                    <div class="fieldwrap"><div class="label">Procedure Category</div> <div class="item"><apex:inputfield value="{!treatment.Procedure_Category__c}"></apex:inputfield></div></div>
                    <div class="fieldwrapfull"><div class="labellong">Procedure Sub-Category</div> <div class="item">(insert custom object)</div></div>
                </div>
            </div>
        </div>
            <div class="mainNav">
                <div class="navitemfirst">
                    <apex:commandLink id="nav1" onmousemove="document.getElementById('arrow1').style.color = '#EA6C1A'" onmouseout="document.getElementById('arrow1').style.color = '#CCCCCC'" action="{!displayInquiry}" reRender="resultPanel">    
                        <h3>Inquiry </h3>
                    </apex:commandLink>
                    <div class="lucida" id="arrow1">&#187;</div><br /><br />
                </div>
                <div class="navitem">
                    <apex:commandLink id="nav2" onmousemove="document.getElementById('arrow2').style.color = '#EA6C1A'" onmouseout="document.getElementById('arrow2').style.color = '#CCCCCC'" action="{!displayInteraction}" reRender="resultPanel">
                        <h3>Interaction </h3>
                    </apex:commandLink>
                    <div class="lucida" id="arrow2">&#187;</div><br /><br />
                </div>
                <div class="navitem">
                    <apex:commandLink id="nav3" onmousemove="document.getElementById('arrow3').style.color = '#EA6C1A'" onmouseout="document.getElementById('arrow3').style.color = '#CCCCCC'" action="{!displayTravel}" reRender="resultPanel">
                        <h3>Travel </h3>
                    </apex:commandLink>
                    <div class="lucida" id="arrow3">&#187;</div><br /><br />
                </div>
                <div class="navitem">
                    <apex:commandLink id="nav4" onmousemove="document.getElementById('arrow4').style.color = '#EA6C1A'" onmouseout="document.getElementById('arrow4').style.color = '#CCCCCC'" action="{!displayPostOp}" reRender="resultPanel">
                        <h3>Post-Op </h3>
                     </apex:commandLink>
                    <div class="lucida" id="arrow4">&#187;</div><br /><br />
                </div>
            </div>                      
        </div>  
    </div>
</div>

<apex:outputPanel id="resultPanel">
    <apex:outputPanel rendered="{!inquiryFlag}">
        <div class="twisty open">   
            <div class="orangeheader">
                <span class="orangeheadertitle">Inquiry</span>
                <span class="orangeheaderoption"><a href="#" style="color:#fff"></a></span>
            </div><br />
            <div class="treatmentfieldwrapper">
                <div class="treatmentform">
                    <div class="fieldwrapper">
                        <div class="fieldwrap"><div class="label">Treatment Event #</div> <div class="item"><a href="/{!treatment.id}" target="_blank" >{!treatment.Name}</a></div></div>
                    </div>
                </div>
                <div class="fieldheading">Inquiry Sourcing</div>
                <div class="treatmentform">
                    <div class="fieldwrapper">                  
                        <div class="fieldwrap"><div class="label">Inquiry Source</div> <div class="item"><apex:inputfield value="{!treatment.Inquiry_Source__c}"></apex:inputfield></div></div>
                        <div class="fieldwrap"><div class="label">Primary Campaign Source</div> <div class="item">(insert custom object)</div></div>
                        <div class="fieldwrap"><div class="label">Related Campaign?</div> <div class="item">(insert custom object)</div></div>
                        <div class="fieldwrap"><div class="label">Advocate Name</div> <div class="item"><apex:inputfield value="{!patient.Advocate_Name__c}"></apex:inputfield></div></div>
                        <div class="fieldwrap"><div class="label">Advocate Phone</div> <div class="item"><apex:inputfield value="{!patient.Advocate_Phone__c}"></apex:inputfield></div></div>
                        <div class="fieldwrap"><div class="label">Advocate Email</div> <div class="item"><apex:inputfield value="{!patient.Advocate_Email__c}"></apex:inputfield></div></div>
                    </div>
                </div>
                <div class="fieldheading">Inquiry Information</div>
                <div class="treatmentform">
                    <div class="fieldwrapper">
                        <div class="fieldwrap"><div class="label">Inquiry Quality</div> <div class="item"><apex:inputfield value="{!treatment.Inquiry_Quality__c}"></apex:inputfield></div></div>
                        <div class="fieldwrap"><div class="label">Inquiry Owner</div> <div class="item"><apex:inputfield value="{!treatment.createdBy.Name}"></apex:inputfield></div></div>
                        <div class="fieldwrap"><div class="label">Preferred Form of Contact</div> <div class="item"><apex:inputfield value="{!treatment.Preferred_Form_of_Contact__c}"></apex:inputfield></div></div>
                        <div class="fieldwrap"><div class="label">Time Zone</div> <div class="item"><apex:inputfield value="{!patient.Time_Zone__c}"></apex:inputfield></div></div>
                        <div class="fieldwrap"><div class="label">Best Time to Contact</div> <div class="item"><apex:inputfield value="{!treatment.Best_Time_to_Contact__c}"></apex:inputfield></div></div>
                        <div class="fieldwrap"><div class="label">Preferred Language</div> <div class="item"><apex:inputfield value="{!patient.Preferred_Language__c}"></apex:inputfield></div></div>                        
                    </div>
                </div>
                <div class="fieldheading">Scheduling Information</div>
                <div class="treatmentform">
                    <div class="fieldwrapper">
                        <div class="fieldwrap"><div class="label">Decision Timing</div> <div class="item"><apex:inputfield value="{!treatment.Decision_Timeframe__c}"></apex:inputfield></div></div>
                        <div class="fieldwrap"><div class="label">Preferred Destination</div> <div class="item"><apex:inputfield value="{!treatment.Preferred_Destination__c}"></apex:inputfield></div></div>
                        <div class="fieldwrap"><div class="label">Financing Required?</div> <div class="item"><apex:inputfield value="{!treatment.Financing_Reguired__c}"></apex:inputfield></div></div> 
                        <div class="fieldwrap"><div class="label">Preferred Travel Date: Depart</div> <div class="item"><apex:inputField value="{!treatment.Preferred_Travel_Date_From__c}"/></div></div>
                        <div class="fieldwrap"><div class="label">Preferred Travel Date: Return</div> <div class="item"><apex:inputField value="{!treatment.Preferred_Travel_Date_To__c}"/></div></div>
                    </div>
                </div>
                <div class="fieldheading">Procedure Date</div>
                <div class="treatmentform">
                    <div class="fieldwrapper">
                        <div class="fieldwrap"><div class="label">Description</div> <div class="item">(insert custom object)</div></div>
                        <div class="fieldwrap"><div class="label">Assign using active assignment rule</div> <div class="item">(insert custom object)</div></div>            
                    </div>
                </div>              
                <div class="fieldheading">Payment Information</div>
                <div class="treatmentform">
                    <div class="fieldwrapper">
                        <div class="fieldwrap"><div class="label">Payment Type</div> <div class="item"><apex:inputfield value="{!treatment.Payment_Type__c} "></apex:inputfield></div></div>
                        <div class="fieldwrap"><div class="label">Deposit Required</div> <div class="item"><apex:inputfield value="{!treatment.Deposit_Required__c} "></apex:inputfield></div></div>   
                    </div>
                </div>                       
                <div class="fieldheading">Demographics</div>
                <div class="treatmentform">
                    <div class="fieldwrapper">
                       <div class="fieldwrap"><div class="label">Age</div> <div class="item">(insert custom object)</div></div>
                        <div class="fieldwrap"><div class="label">Male or Female</div> <div class="item"><apex:inputfield value="{!patient.Gender__c}"></apex:inputfield></div></div>
                        <div class="fieldwrap"><div class="label">Nationality</div> <div class="item">(insert custom object)</div></div>
                        <div class="fieldwrap"><div class="label">Country Born</div> <div class="item"><apex:inputfield value="{!patient.Country__c}"></apex:inputfield></div></div>
                    </div>
                </div>              
            </div>
        </div>
    </apex:outputPanel>
    
    <apex:outputPanel rendered="{!interactionFlag}">
        <div class="twisty open">   
            <div class="orangeheader">
                <span class="orangeheadertitle">Interaction</span>
                <span class="orangeheaderoption"><a href="#" style="color:#fff"></a></span>
            </div><br />
            <div class="treatmentfieldwrapper">
                <div class="treatmentform">
                    <div class="fieldwrapper">
                        <div class="fieldwrap"><div class="label">Treatment Event #</div> <div class="item"><a href="/{!treatment.id}" target="_blank" >{!treatment.Name}</a></div></div>
                    </div>
                </div>
                <div class="fieldheading">Interaction Information</div>
                <div class="treatmentform">
                    <div class="fieldwrapper">                      
                        <div class="fieldwrap"><div class="label">Probability</div> <div class="item">(insert custom object)</div></div>
                        <div class="fieldwrap"><div class="label">Request Summary</div> <div class="item">(insert custom object)</div></div>
                        <div class="fieldwrap"><div class="label">Hospital Destination</div> <div class="item"><apex:inputfield value="{!treatment.Hospital_Destination__c}"></apex:inputfield></div></div>
                        <div class="fieldwrap"><div class="label">Procedure Amount</div> <div class="item"><apex:inputfield value="{!treatment.Amount__c}"></apex:inputfield></div></div>
                        <div class="fieldwrap"><div class="label">Close Date</div> <div class="item"><apex:inputField value="{!treatment.Close_Date__c }"/></div></div>
                        <div class="fieldwrap"><div class="label">Case Manager's Feel</div> <div class="item"><apex:inputfield value="{!treatment.Case_Manager_s_Feel__c}"></apex:inputfield></div></div>
                    </div>
                </div>
                <div class="fieldheading">Scheduling Information</div>
                <div class="treatmentform">
                    <div class="fieldwrapper">
                        <div class="fieldwrap"><div class="label">Decision Timing</div> <div class="item"><apex:inputfield value="{!treatment.Decision_Timeframe__c}"></apex:inputfield></div></div>
                        <div class="fieldwrap"><div class="label">Preferred Destination</div> <div class="item"><apex:inputfield value="{!treatment.Preferred_Destination__c}"></apex:inputfield></div></div>
                        <div class="fieldwrap"><div class="label">Financing Required?</div> <div class="item"><apex:inputfield value="{!treatment.Financing_Reguired__c}"></apex:inputfield></div></div> 
                        <div class="fieldwrap"><div class="label">Preferred Travel Date: Depart</div> <div class="item"><apex:outputField value="{!treatment.Preferred_Travel_Date_From__c}"/></div></div>
                        <div class="fieldwrap"><div class="label">Preferred Travel Date: Return</div> <div class="item"><apex:outputField value="{!treatment.Preferred_Travel_Date_To__c}"/></div></div>
                    </div>
                </div>
                <div class="fieldheading">Procedure Date</div>
                <div class="treatmentform">
                    <div class="fieldwrapper">
                        <div class="fieldwrap"><div class="label">Description</div> <div class="item">(insert custom object)</div></div>
                        <div class="fieldwrap"><div class="label">Assign using active assignment rule</div> <div class="item">(insert custom object)</div></div>            
                    </div>
                </div>              
                <div class="fieldheading">Payment Information</div>
                <div class="treatmentform">
                    <div class="fieldwrapper">
                        <div class="fieldwrap"><div class="label">Payment Type</div> <div class="item"><apex:inputfield value="{!treatment.Payment_Type__c}"></apex:inputfield></div></div>
                        <div class="fieldwrap"><div class="label">Deposit Required</div> <div class="item"><apex:inputfield value="{!treatment.Deposit_Required__c}"></apex:inputfield></div></div>   
                    </div>
                </div>                      
            </div>
        </div>
        <div class="tabs">
            <apex:commandLink id="tab1" styleClass="tab" style="color:#EA6C1A;" onmousemove="document.getElementById('tab1').style.color = '#EA6C1A'" onmouseout="document.getElementById('tab1').style.color = '#CCCCCC'" action="{!displayIncident}" reRender="TabPanel"><h3>Incident</h3></apex:commandLink>
            <apex:commandLink id="tab2" styleClass="tab" onmousemove="document.getElementById('tab2').style.color = '#EA6C1A'" onmouseout="document.getElementById('tab2').style.color = '#CCCCCC'" action="{!displayFeatures}" reRender="TabPanel"><h3>Medical Profile and Record</h3></apex:commandLink>
            <apex:commandLink id="tab3" styleClass="tab" onmousemove="document.getElementById('tab3').style.color = '#EA6C1A'" onmouseout="document.getElementById('tab3').style.color = '#CCCCCC'" action="{!displayQuotes}" reRender="TabPanel"><h3>Quotes</h3></apex:commandLink>
            <apex:commandLink id="tab4" styleClass="tab" onmousemove="document.getElementById('tab4').style.color = '#EA6C1A'" onmouseout="document.getElementById('tab4').style.color = '#CCCCCC'" action="{!displayPayments}" reRender="TabPanel"><h3>Payments</h3></apex:commandLink>
        </div>
        <apex:outputPanel id="TabPanel">
            <apex:outputPanel id="IncidentPanel" rendered="{!IncidentFlag}">
                <div id="tab1box" class="tabbox" style="display:block;">
                    <div class="treatmentfieldwrapper">
                        <div class="fieldheading">Incident Detail</div>                        
                        <div class="treatmentformwhite">
                            <div class="fieldwrapper">          
                                <div class="fieldwrap"><div class="label">Incident Number</div> <div class="item"><apex:inputfield value="{!incident.Name}"></apex:inputfield></div></div>
                                <div class="fieldwrap"><div class="label">Contact Name (lookup)</div> <div class="item">(insert custom object)</div></div>
                                <div class="fieldwrap"><div class="label">Account Name</div> <div class="item">(insert custom object)</div></div>
                                <div class="fieldwrap"><div class="label">Type</div> <div class="item">(insert custom object)</div></div>
                                <div class="fieldwrap"><div class="label">Category</div> <div class="item">(insert custom object)</div></div>
                                <div class="fieldwrap"><div class="label">Subject</div> <div class="item">(insert custom object)</div></div>
                                <div class="fieldwrap"><div class="label">Date Time Opened</div> <div class="item">(insert custom object)</div></div>
                                <div class="fieldwrap"><div class="label">Created By</div> <div class="item">(insert custom object)</div></div>
                                <div class="fieldwrap"><div class="label">Incident Owner</div> <div class="item">(insert custom object)</div></div>
                                <div class="fieldwrap"><div class="label">Status</div> <div class="item">(insert custom object)</div></div>
                                <div class="fieldwrap"><div class="label">Due Date</div> <div class="item">(insert custom object)</div></div>
                                <div class="fieldwrap"><div class="label">Contact Phone</div> <div class="item">(insert custom object)</div></div>
                                <div class="fieldwrap"><div class="label">Contact Email</div> <div class="item">(insert custom object)</div></div>
                                <div class="fieldwrap"><div class="label">Date Time Closed</div> <div class="item">(insert custom object)</div></div>
                                <div class="fieldwrap"><div class="label">Last Modified By  </div> <div class="item">(insert custom object)</div></div>
                            </div>

                        </div>                      
                        <div class="fieldheading">Open Activities</div>
                        <div class="treatmentformwhite">
                            <div class="fieldwrapper">
                                <div class="fieldwrap"><div class="label"></div> <div class="item"></div></div>
                            </div>
                        </div>
                        <div class="fieldheading">Activity History</div>
                        <div class="treatmentformwhite">
                            <div class="fieldwrapper">
                                <div class="fieldwrap"><div class="label"></div> <div class="item"></div></div>
                            </div>
                        </div>
                        <div class="fieldheading">Attachments</div>
                        <div class="treatmentformwhite">
                            <div class="fieldwrapper">
                                <div class="fieldwrap"><div class="label"></div> <div class="item"></div></div>
                            </div>
                        </div>
                        <div class="fieldheading">Incident History</div>
                        <div class="treatmentformwhite">
                            <div class="fieldwrapper">
                                <div class="fieldwrap"><div class="label"></div> <div class="item"></div></div>
                            </div>
                        </div>                                                      
                    </div>      
                </div>  
            </apex:outputPanel>
            <apex:outputPanel id="FeaturesPanel" rendered="{!FeaturesFlag}">
                <div id="tab2box" class="tabbox" style="display:block;">
                    <div class="treatmentfieldwrapper">
                        <div class="fieldheading">Features</div>
