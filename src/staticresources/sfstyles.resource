}
.pipelinechartscol2 {
	padding:20px;
	border:1px solid #ccc;
	background-color:#eee;
	float:left;
	margin-bottom:20px;
	margin-top:30px;
}
.chartheader{
	background-color:#ccc;
	line-height: 30px;
}
.chartheader a{
	color:#000;
	margin-right:20px;
	padding: 5px;
}
.chartdetailsodd a{
	color:#EA6C1A;
